inherited TituloService: TTituloService
  OldCreateOrder = True
  OnCreate = DataModuleCreate
  inherited fdConexao: TFDConnection
    Connected = True
  end
  object qryTitulo: TFDQuery
    Connection = fdConexao
    SQL.Strings = (
      'select * from titulo t where t.id = :ID')
    Left = 144
    Top = 96
    ParamData = <
      item
        Name = 'ID'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
  end
  object qryCobConf: TFDQuery
    Connection = fdConexao
    SQL.Strings = (
      
        '/*7340 - sel_cobconf_cedente - selecionar as informacoes do cede' +
        'nte do boleto*/'
      'select estab.*, cidade.nome CIDADENOME, uf.sigla, '
      'PORTADOR.BANCO, PORTADOR.AGENCIA, PORTADOR.CONTA, '
      'cobconf.*'
      ' from cobconf'
      
        'inner join estab on (estab.id = coalesce(cobconf.estab_id, coale' +
        'sce(cobconf.infoestab_id, :ESTAB_ID)))'
      'left join cidade on cidade.id = estab.cidade_id'
      'left join uf on uf.id = cidade.uf_id'
      'left join PORTADOR on PORTADOR.ID = COBCONF.PORTADOR_ID'
      ''
      'where cobconf.id = :COBCONF_ID')
    Left = 80
    Top = 96
    ParamData = <
      item
        Name = 'ESTAB_ID'
        DataType = ftInteger
        ParamType = ptInput
        Value = 3
      end
      item
        Name = 'COBCONF_ID'
        DataType = ftInteger
        ParamType = ptInput
        Value = 3
      end>
  end
end
